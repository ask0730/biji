                                    str_是否已经登录=Image.Exists({"wnd":[{"cls":"Chrome_WidgetWin_1","title":"中华数字书苑-中华数字文明精粹 - Google Chrome","app":"chrome"}]},{"x": 0, "y": 0, "width": 0, "height": 0},@res"96677010-f3a8-11ed-b9be-4d171ee671f6.png",0.9,10000,{"bContinueOnError": false, "iDelayAfter": 300, "iDelayBefore": 200, "bSetForeground": true, "sMatchType":"GrayMatch"})
                                Select Case str_是否已经登录 
                                    Case true 
                                        str_验证码报错存在 = false
                                        str_是否已经登录 = false
                                        value[3] = "可用"
                                        File.Append(str_日志文件+"\\"+str_日期+".log","["+Time.Format(Time.Now(),"yyyy-mm-dd hh:mm:ss")+"]：【需要登录】["+str_名称+"]数据结论：该网页二次登录成功，可以正常使用。\n","utf-8")
                                    Case Else 
                                        str_登录是否成功 = false
                                        str_验证码报错存在 = false
                                        value[3] = "不可用"
                                        File.Append(str_日志文件+"\\"+str_日期+".log","["+Time.Format(Time.Now(),"yyyy-mm-dd hh:mm:ss")+"]：【需要登录】["+str_名称+"]数据结论：该网页未能正常打开。\n","utf-8")
                                    End Select
                            End Select
                    Loop
            
            //分支26：中国报纸资源全文数据库[http://www.apabi.com/clcn]   
                Case "中国报纸资源全文数据库" 
                    str_验证码报错存在 = true
                    str_时间 = Time.Format(Time.Now(),"yyyymmddhhmmss")
                    Do While str_验证码报错存在=true
                        http_网址1 = WebBrowser.GoURL(http_网址,str_网址,true,{},3000,{"bContinueOnError":false,"iDelayAfter":300,"iDelayBefore":2000})
                        #icon("@res:55db70a0-f2f4-11ed-93af-d729429c17d6.png")
                        str_是否存在=Image.Exists({"wnd":[{"cls":"Chrome_WidgetWin_1","title":"中华数字书苑-中华数字文明精粹-用户登录 - Google Chrome","app":"chrome"}]},{"x": 0, "y": 0, "width": 0, "height": 0},@res"55db70a0-f2f4-11ed-93af-d729429c17d6.png",0.9,10000,{"bContinueOnError": false, "iDelayAfter": 300, "iDelayBefore": 200, "bSetForeground": true, "sMatchType":"ColorMatch"})
                        Select Case str_是否存在 
                                Case true 
                                    Keyboard.InputText(@ui"输入控件<input>41",str_测试账号,true,20,10000,{"bContinueOnError": true, "iDelayAfter": 300, "iDelayBefore": 3000, "bSetForeground": true, "sSimulate": "message", "bValidate": false, "bClickBeforeInput": true})
                                    Keyboard.InputText(@ui"输入控件<input>42",str_测试密码,true,20,10000,{"bContinueOnError": false, "iDelayAfter": 300, "iDelayBefore": 500, "bSetForeground": true, "sSimulate": "message", "bValidate": false, "bClickBeforeInput": true})
                                    UiElement.ScreenCapture(str_验证文件+"\\"+str_时间+".png",@ui"图像<img>5",{"x":0,"y":0,"width":0,"height":0},{"bContinueOnError":false,"iDelayAfter":300,"iDelayBefore":200})
                                    Clipboard.SetImage(str_验证文件+"\\"+str_时间+".png")
                                    Keyboard.Press("T", "press", ["Ctrl"],{"iDelayAfter": 300, "iDelayBefore": 200, "sSimulate": "simulate"})
                                    Keyboard.InputText(@ui"可编辑文本_地址和搜索栏1","https://pearocr.com/#/",true,20,10000,{"bContinueOnError": false, "iDelayAfter": 300, "iDelayBefore": 500, "bSetForeground": true, "sSimulate": "message", "bValidate": false, "bClickBeforeInput": false})
                                    Keyboard.Press("Enter", "press", [],{"iDelayAfter": 300, "iDelayBefore": 200, "sSimulate": "simulate"})
                                    Mouse.Action(@ui"文本<span>_读剪贴板3","left","click",10000,{"bContinueOnError": false, "iDelayAfter": 300, "iDelayBefore": 200, "bSetForeground": true, "sCursorPosition": "Center", "iCursorOffsetX": 0, "iCursorOffsetY": 0, "sKeyModifiers": [],"sSimulate": "simulate", "bMoveSmoothly": false})
                                    Mouse.Action(@ui"文本<span>_复制2","left","click",10000,{"bContinueOnError": false, "iDelayAfter": 300, "iDelayBefore": 200, "bSetForeground": true, "sCursorPosition": "Center", "iCursorOffsetX": 0, "iCursorOffsetY": 0, "sKeyModifiers": [],"sSimulate": "simulate", "bMoveSmoothly": false})
                                    Keyboard.Press("W", "press", ["Ctrl"],{"iDelayAfter": 300, "iDelayBefore": 200, "sSimulate": "simulate"})
                                    Mouse.Action(@ui"输入控件<input>43","left","click",10000,{"bContinueOnError": false, "iDelayAfter": 300, "iDelayBefore": 200, "bSetForeground": true, "sCursorPosition": "Center", "iCursorOffsetX": 0, "iCursorOffsetY": 0, "sKeyModifiers": [],"sSimulate": "simulate", "bMoveSmoothly": false})
                                    Keyboard.Press("V", "press", ["Ctrl"],{"iDelayAfter": 300, "iDelayBefore": 200, "sSimulate": "simulate"})
                                    File.DeleteFile(str_验证文件+"\\"+str_时间+".png")
                                    Keyboard.Press("Enter", "press", [],{"iDelayAfter": 300, "iDelayBefore": 200, "sSimulate": "simulate"})
                                    #icon("@res:298cbd80-f3ab-11ed-b9be-4d171ee671f6.png")
                                    str_是否已经登录=Image.Exists({"wnd":[{"cls":"Chrome_WidgetWin_1","title":"中华数字书苑-中华数字文明精粹-用户登录 - Google Chrome","app":"chrome"}]},{"x": 0, "y": 0, "width": 0, "height": 0},@res"298cbd80-f3ab-11ed-b9be-4d171ee671f6.png",0.9,10000,{"bContinueOnError": false, "iDelayAfter": 300, "iDelayBefore": 200, "bSetForeground": true, "sMatchType":"GrayMatch"})
                                    Select Case str_是否已经登录 
                                            Case true 
                                                str_验证码报错存在 = false
                                                value[3] = "可用"
                                                File.Append(str_日志文件+"\\"+str_日期+".log","["+Time.Format(Time.Now(),"yyyy-mm-dd hh:mm:ss")+"]：【需要登录】["+str_名称+"]数据结论：该网页可以正常打开。\n","utf-8")
                                            Case Else 
                                    End Select
                                Case Else 
                                    str_是否存在 = false
                                    #icon("@res:96677010-f3a8-11ed-b9be-4d171ee671f6.png")
                                    str_是否已经登录=Image.Exists({"wnd":[{"cls":"Chrome_WidgetWin_1","title":"中华数字书苑-中华数字文明精粹 - Google Chrome","app":"chrome"}]},{"x": 0, "y": 0, "width": 0, "height": 0},@res"96677010-f3a8-11ed-b9be-4d171ee671f6.png",0.9,10000,{"bContinueOnError": false, "iDelayAfter": 300, "iDelayBefore": 200, "bSetForeground": true, "sMatchType":"GrayMatch"})
                                Select Case str_是否已经登录 
                                    Case true 
                                        str_验证码报错存在 = false
                                        str_是否已经登录 = false
                                        value[3] = "可用"
                                        File.Append(str_日志文件+"\\"+str_日期+".log","["+Time.Format(Time.Now(),"yyyy-mm-dd hh:mm:ss")+"]：【需要登录】["+str_名称+"]数据结论：该网页二次登录成功，可以正常使用。\n","utf-8")
                                    Case Else 
                                        str_登录是否成功 = false
                                        str_验证码报错存在 = false
                                        value[3] = "不可用"
                                        File.Append(str_日志文件+"\\"+str_日期+".log","["+Time.Format(Time.Now(),"yyyy-mm-dd hh:mm:ss")+"]：【需要登录】["+str_名称+"]数据结论：该网页未能正常打开。\n","utf-8")
                                    End Select
                            End Select
                    Loop 
                
                Case "状态" 
                        File.Append(str_日志文件+"\\"+str_日期+".log","["+Time.Format(Time.Now(),"yyyy-mm-dd hh:mm:ss")+"]："+"【开始登录各种网址测试】\n","utf-8")
                Case Else   
                        value[3] = "初始不可用"  
                        File.Append(str_日志文件+"\\"+str_日期+".log","["+Time.Format(Time.Now(),"yyyy-mm-dd hh:mm:ss")+"]："+"【初始不可用】\n","utf-8")
                
                End Select 
    End Select
TracePrint("登陆网站："+value[3])
Next

Keyboard.Press("W", "press", ["Ctrl"],{"iDelayAfter": 300, "iDelayBefore": 200, "sSimulate": "simulate"})
