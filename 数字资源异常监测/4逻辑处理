//str_数据抓取网站 = "https://www.clcn.net.cn/resources/default/index?mode=external_link#selector-category-key"
//变量定义
Dim str_网址,str_名称,str_类型,str_是否存在,str_登录是否成功,str_是否已经登录,arr_第一页数据,arr_第二页数据,arr_第一次合并数据,arr_第三页数据,http_网址1,str_验证码报错存在,bRet,hWeb,temp

//初始化变量
str_是否存在 = flase
str_时间 = Time.Format(Time.Now(),"yyyymmddhhmmss")

//str_登录是否成功 = flase
//str_是否已经登录 = false
//遍历并处理数据
File.Append(str_日志文件+"\\"+str_日期+".log","["+Time.Format(Time.Now(),"yyyy-mm-dd hh:mm:ss")+"]："+"逻辑处理开始\n","utf-8")
For Each value In arr_最终数据
    str_网址 = value[0]
    str_名称 = value[1]
    str_类型 = value[2]
    TracePrint("登陆网站："+str_名称)
TracePrint("登陆网站："+str_网址)
File.Append(str_日志文件+"\\"+str_日期+".log","["+Time.Format(Time.Now(),"yyyy-mm-dd hh:mm:ss")+"]："+"===========================================================\n"+"["+Time.Format(Time.Now(),"yyyy-mm-dd hh:mm:ss")+"]："+"数据 分析： "+"["+str_名称+"]["+str_网址+"]"+str_类型+"\n","utf-8")
    //第一次类型分支选择（试用、非试用）
    Select Case str_类型 
        Case "试用" 
        File.Append(str_日志文件+"\\"+str_日期+".log","["+Time.Format(Time.Now(),"yyyy-mm-dd hh:mm:ss")+"]："+"【试用忽略】["+str_名称+"]数据数据结论：本数据为试用数据，可以忽略。\n","utf-8")
        value[3] = "试用中"
        Case Else
            //第二次名称分支选择（名称对应的操作）
            
            Select Case str_名称 
            
            //不需要登陆
            //分支1：北京记忆 [http://www.bjmem.com.cn/]
            
