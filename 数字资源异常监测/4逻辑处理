                            File.Append(str_日志文件+"\\"+str_日期+".log","["+Time.Format(Time.Now(),"yyyy-mm-dd hh:mm:ss")+"]："+"【直接打开】"+"["+str_名称+"]数据结论：该网页可以正常打开。\n","utf-8")
